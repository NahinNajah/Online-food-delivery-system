            <Button fx:id="yesButton" layoutX="177.0" layoutY="231.0" mnemonicParsing="false" onAction="#YesButtonHandler" text="YES" />
            <Label contentDisplay="TOP" layoutX="34.0" layoutY="82.0" prefHeight="90.0" prefWidth="542.0" text="DO YOU WANT TO CONFIRM THE PLACE ORDER" textFill="#ef0c0c">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Button fx:id="noButton" layoutX="324.0" layoutY="231.0" mnemonicParsing="false" onAction="#NoButtonHandler" text="NO" />
            <Button fx:id="goButton" layoutX="212.0" layoutY="285.0" mnemonicParsing="false" onAction="#GoButtonHandler" text="GO TO HOME PAGE" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
